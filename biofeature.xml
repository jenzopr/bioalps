<?xml version="1.0" ?>
<alps>
  <doc>A list of biological features. Technically, containers for feature start and end positions on a sequence in an arbitrary coordinate system.</doc>
  
  <!-- base elements -->
  <descriptor id="feature" type="semantic">
    <doc>Refers to a feature of the context.</doc>
  </descriptor>
  
  <descriptor id="start" type="semantic">
    <doc>The start of the feature.</doc>
  </descriptor>
  
  <descriptor id="end" type="semantic">
    <doc>The end of the feature.</doc>
  </descriptor>
  
  <descriptor id="strand" type="semantic">
    <doc>The strand on which the feature is located. If not present, the feature has no strandness.</doc>
  </descriptor>
  
  <descriptor id="type" type="semantic">
    <doc>A feature type, possibly from an ontology (e.g. so:0001077). Free text works as well.</doc>
    <link rel="help" href="http://bioportal.bioontology.org/ontologies/SO" />
  </descriptor>
  
  <descriptor id="description" type="semantic">
    <doc>An additional description.</doc>
  </descriptor>
  
  <descriptor id="precondition" type="semantic">
    <doc>A precondition on which the features presence might depend.</doc>
  </descriptor>
  
  <!-- state transitions -->
  
  <descriptor id="bioentity" type="safe" rt="http://alps.example.org/bioentity.xml#bioentity">
    <doc>Refers to the biological entity in the current context.</doc>
  </descriptor>
  
  <descriptor href="http://alps.io/iana/relations#up" />
  <descriptor href="http://alps.io/iana/relations#collection" />
  <descriptor href="http://alps.io/iana/relations#next" />
  <descriptor href="http://alps.io/iana/relations#prev" />
  <descriptor href="http://alps.io/iana/relations#first" />
  <descriptor href="http://alps.io/iana/relations#last" />
  <descriptor href="http://alps.io/iana/relations#describedby" />
  <descriptor href="http://als.io/iana/relations.xml#related" />
  <descriptor href="http://alps.io/iana/relations.xml#create" />
  <descriptor href="http://alps.io/iana/relations.xml#edit" />
  <descriptor href="http://alps.io/iana/relations.xml#search" />
<alps>